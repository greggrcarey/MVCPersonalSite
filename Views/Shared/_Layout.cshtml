<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="description" content="Personal Site for Gregg R Carey, .NET Web Developer" />
    <meta name="keywords" content=".NET, developer, ASP, MVC, mobile, responsive, jQuery, javascript" />
    <link href='http://fonts.googleapis.com/css?family=Source+Sans+Pro|Roboto|Open+Sans:400italic,600italic,600,400' rel='stylesheet' type='text/css'>
    <link type="image/ico" href="Content/favicon.ico" rel="shortcut icon" />
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/StyleSheet.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.7.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/modernizr-2.5.3.js")" type="text/javascript"></script>
    <script src="//platform.linkedin.com/in.js" type="text/javascript"></script>
    <script type="text/javascript" charset="utf-8">
        $(function () {
            $('#menu ul li').each(function () {
                if ($(this).find('a').attr('href') === window.location.pathname)
                {
                    $(this).addClass('selected');
                }
            });
        });
    </script>
     <script>
         (function (i, s, o, g, r, a, m) {
             i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
                 (i[r].q = i[r].q || []).push(arguments)
             }, i[r].l = 1 * new Date(); a = s.createElement(o),
             m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
         })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');

         ga('create', 'UA-42934720-1', 'greggrcarey.com');
         ga('send', 'pageview');

    </script>

</head>
<body>
    <div id="wrapper">
        <div class="page">
            <header>
                <div id="title">
                    <h1>Gregg Carey | .Net Developer</h1>
                </div>
                <nav id="menu">
                    <ul>
                        <li>@Html.ActionLink("Home", "Index", "Home")</li>
                        <li>@Html.ActionLink("Resume", "Resume", "Home")</li>
                        <li>@Html.ActionLink("Projects", "Index", "Projects")</li>
                        <li>@Html.ActionLink("Contact", "Contact", "Home")</li>
                        
                    </ul>
                </nav>
                <div style="clear: both"></div>
            </header>
            <section id="main">
                @RenderBody()
            </section>
            <footer>

                <div id="logindisplay">
                    &copy;2013 greggrcarey.com
                    @Html.Partial("_LogOnPartial")
                </div>

            </footer>
            <div style="clear: both"></div>
        </div>
    </div>
</body>
</html>
